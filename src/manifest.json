{
  "name": "gitmoji-chrome-extension",
  "version": "1.0",
  "description": "Convert gitmoji shortcode to the corresponding emoji",
  "permissions": ["tabs", "declarativeContent", "storage"],
  "browser_action": {
    "default_popup": "./popup.html"
  },
  "default_icon": {
    "72": "icon-72x72.png"
  },
  "manifest_version": 2
}